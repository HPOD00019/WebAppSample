{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "RabbitMQ": {
        "Host": "localhost",
        "Port": 5672,
        "VirtualHost": "/",
        "Username": "guest",
        "Password": "guest",
        "MassTransit": {
            "PrefetchCount": 16,
            "ConcurrentMessageLimit": 8,
            "RetryPolicy": {
                "Interval": "00:00:05",
                "RetryLimit": 3
            },
            "ConfigureConsumeTopology": true,
            "AutoStart": true
        }
    },
    "AllowedHosts": "*"
}
